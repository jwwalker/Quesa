// Architectures
ARCHS = $(ARCHS_STANDARD)
SDKROOT = macosx


// Build Options
PRECOMPS_INCLUDE_HEADERS_FROM_BUILT_PRODUCTS_DIR = NO


// Linking
PREBINDING = NO
OTHER_LDFLAGS = "../../Libraries/Mac/Static_Modern/$CONFIGURATION/libQuesa.a"


// GCC Code Generation
GCC_ENABLE_FIX_AND_CONTINUE = NO


// GCC Warnings
GCC_WARN_ABOUT_RETURN_TYPE = YES
GCC_WARN_UNUSED_VARIABLE = YES


// Packaging
PRODUCT_NAME = Binify3DMF


// Search Paths
ALWAYS_SEARCH_USER_PATHS = NO
HEADER_SEARCH_PATHS = "../../Includes/Quesa"


// Preprocessing
GCC_PREPROCESSOR_DEFINITIONS = QUESA_OS_MACINTOSH=1 QUESA_OS_COCOA=1 QUESA_SUPPORT_HITOOLBOX=0
