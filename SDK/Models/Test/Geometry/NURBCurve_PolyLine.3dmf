		2			# order (order 2 = degree 1 = linear)
		8			# numPoints

		# points
		-3  0  1  1	# point 0
		 3  1  1  1	# point 1
		 3 -2  1  1	# point 2
		-3 -2  1  1	# point 3
		-3 -2 -1  1	# point 4
		 3 -2 -1  1	# point 5
		 3  1 -1  1	# point 6
		-3  2 -1  1	# point 7

		# knots
		-9			# ignored
		 0  1  2  3  4  5  6  7
		 9			# ignored
	)
